\n","def sobel_edge_detection(img,vert_mask,horz_mask):\n","  new_img_x=cv2.filter2D(img,-1,vert_mask)\n","  new_img_y=cv2.filter2D(img,-1,horz_mask)\n","  edge_img=np.sqrt(new_img_x**2+new_img_y**2)\n","  edge_img*=255/edge_img.max()\n","  edge_img= np.where(edge_img<50,0,255)\n","  return edge_img\n","\n","def prewitt_edge_detection(img,vert_mask,horz_mask):\n","  new_img_x=cv2.filter2D(img,-1,vert_mask)\n","  new_img_y=cv2.filter2D(img,-1,horz_mask)\n","  edge_img=np.sqrt(new_img_x**2+new_img_y**2)\n","  edge_img*=255/edge_img.max()\n","  edge_img= np.where(edge_img<50,0,255)\n","  return edge_img\n","\n","def run_Q2():\n","  for i in range(0,4):\n","    imgB=plt.imread(path+images[i])\n","    imgB=cv2.filter2D(imgB,-1,kernel)\n","    edge_imgB=sobel_edge_detection(imgB,vert_mask,horz_mask)\n","    #edge_imgB=prewitt_edge_detection(imgB,prewitt_vert_mask,prewitt_horz_mask)\n","    plt.imsave(path+'output/edge_'+images[i],edge_imgB,cmap='gray')\n","    plt.subplot(2,2,i+1)\n","    plt.title(images[i])\n","    plt.imshow(edge_imgB,cmap='gray')\n","  plt.savefig(path+'output/sobel_output')\n","run_Q2()"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 4 Axes>"]},"metadata":{"needs_background":"light"}}]},{"cell_type":"markdown","metadata":{"id":"x3-D4dh5GbwZ"},"source":["**C. Laplacian Operator**"]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":281},"id":"5vOU3Oo3Bm-f","executionInfo":{"status":"ok","timestamp":1636879945781,"user_tz":-330,"elapsed":2150,"user":{"displayName":"SHAH ROHAN GIRISHKUMAR","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"03045170154293814404"}},"outputId":"13e69229-96cc-4a58-fe09-8d1e36e64b4e"},"source":["lap_mask=np.array([[0,1,0],[1,-4,1],[0,1,0]])\n","\n","def laplacian_edge_detection(img,lap_mask):\n","  edge_img=cv2.filter2D(img,-1,lap_mask)\n","  #edge_img*=255/edge_img.max()\n","  t=edge_img.max()\n","  edge_img= np.where(abs(edge_img)<0.27*t,0,255)\n","  lap = np.pad(edge_img, ((0, 1), (0, 1)))\n","  diff_x = lap[:-1, :-1] - lap[:-1, 1:] < 0\n","  diff_y = lap[:-1, :-1] - lap[1:, :-1] < 0\n","  edge_img =  np.logical_or(diff_x, diff_y).astype(float)\n","  return edge_img\n","\n","def run_Q2C():\n","  for i in range(0,4):\n","    imgB=plt.imread(path+images[i])\n","    imgB=cv2.filter2D(imgB,-1,kernel)\n","    edge_imgB=laplacian_edge_detection(imgB,lap_mask)\n","    #plt.imsave(path+'output/edge_'+images[i],edge_imgB,cmap='gray')\n","    plt.subplot(2,2,i+1)\n","    plt.title(images[i])\n","    plt.imshow(edge_imgB,cmap='gray')\n","  plt.savefig(path+'output/laplacian_output')\n","run_Q2C()\n","\n","\n","   "],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 4 Axes>"]},"metadata":{"needs_background":"light"}}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":286},"id":"naJ1uKLooLIy","executionInfo":{"status":"ok","timestamp":1636880283976,"user_tz":-330,"elapsed":705,"user":{"displayName":"SHAH ROHAN GIRISHKUMAR","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"03045170154293814404"}},"outputId":"356bd20b-d458-4680-f682-3248add96d2a"},"source":["imgB=plt.imread(path+images[1])\n","laplacian = cv2.Laplacian(imgB,-1)\n","edge=np.where(abs(laplacian)<0.2,0,255)\n","plt.imshow(edge,cmap='gray')"],"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["<matplotlib.image.AxesImage at 0x7fad6ba61d10>"]},"metadata":{},"execution_count":19},{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"needs_background":"light"}}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"_7J_XLMNobhI","executionInfo":{"status":"ok","timestamp":1636880130337,"user_tz":-330,"elapsed":385,"user":